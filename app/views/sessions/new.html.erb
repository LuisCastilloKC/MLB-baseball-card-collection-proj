<h1>Login</h1>

<%= flash[:error] if flash[:error] %>


    <%= form_with url: login_path do |f| %>
        <%= f.label :username %>
        <%= f.text_field :username %><br>

        <%= f.label :password %>
        <%= f.password_field :password %><br>

        <%= f.submit "Log In" %>
        
    <% end %>

